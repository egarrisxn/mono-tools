{
  "name": "mono-tools",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --check \"**/*.{js,jsx,ts,tsx,md}\"",
    "format:fix": "prettier --write \"**/*.{js.,jsx,ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "turbo": "^2.4.4",
    "typescript": "5.8.2"
  },
  "packageManager": "pnpm@10.7.0",
  "engines": {
    "node": ">=20"
  }
}
